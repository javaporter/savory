extends /layout

block subnav
	.contain-to-grid.show-for-large-up(data-magellan-expedition='fixed')
		nav.top-bar(data-topbar, role='navigation')
			.top-bar-section
				ul
					+platform-subnav

				ul.right
					li.divider
					li
						a.cart-items(href='#', data-reveal-id='checkout')
							i.fa.fa-shopping-cart
							| &nbsp; View Cart:&nbsp;
							span $0